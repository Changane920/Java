package N_FL_EX;

public class N_FL_EX_20 {
	public static void main(String[] args) {
		char alpha = 'A';
		for(int row = 1; row <= 5; row++) {
			for(int word = 5; word >= row; word--) {
				for(char word2 = alpha; word2 <= 65; word2++) {
					System.out.print(word2 + " ");
				}
			}
			System.out.println();
		}
	}
}
