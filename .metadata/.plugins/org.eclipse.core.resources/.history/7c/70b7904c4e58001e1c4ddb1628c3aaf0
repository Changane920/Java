package N_FL_EX;

public class N_FL_EX_8 {
	public static void main(String[] args) {
		for(int row = 1; row <= 5; row++) {
			for(int space = 0; space <= 4; space++) {
				System.out.print(" ");
			}
			for(int digit = 1; digit <= row; digit++) {
				System.out.print(digit);
			}
			System.out.println();
		}
	}
}
