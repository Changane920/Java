package FL_EX;

import java.util.Scanner;

public class Login_Attempt {
	
	static boolean flag = false;
	public static void main(String[] args) {
		if(!flag) {
			System.out.println("Success");
		}
		else {
			System.out.println("Fail");
		}
	}
	
	static boolean checkPass(String name, String pass) {
		String uname = "chan";
		String upass = "chan12";
		int count = 0;
		Scanner sc = new Scanner(System.in);

		do {
			name = sc.next();
			pass = sc.next();
			count++;
			flag = true;
		}
		while(count < 5 || uname == name && upass == pass);
		flag = false;
		return flag;
	}
}
