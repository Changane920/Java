package N_FL_EX;

public class N_FL_EX_20 {
	public static void main(String[] args) {
		char alpha = 'A';
		int num = 70;
		for(int row = 1; row <= 5; row++) {
				for(char word2 = alpha; word2 <= num; word2++) {
					System.out.print(word2 + " ");
				}
				num--;
			System.out.println();
		}
	}
}
