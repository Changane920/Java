package Array_EX;

import java.util.Arrays;

public class A_EX_5_ {
	public static void main(String[] args) {
		int[] array = {1,2,3,4,5};
		int[] array2 = new int[array.length];
		for(int i = 0; i < array.length; i++) {
			if(i == array[2]) {
				continue;
			}
			array2[i] = array[i];
		}
		
		for(int i = 0; i < array2.length; i++) {
			if(array[i] == 0) {
				continue;
			}
			System.out.println(array2[i]);
		}
	}
}
