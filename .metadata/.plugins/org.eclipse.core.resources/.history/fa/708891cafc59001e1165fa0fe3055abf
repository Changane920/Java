package N_FL_EX;

public class N_FL_EX_19 {
	public static void main(String[] args) {
		int num = 65;
		char alpha = 'A';
		
		for(int row = 1; row <= 8; row++) {
			if(num >= 65 && num <= 90) {
				for(char word = alpha; word <= num; word++) {
					System.out.print(word + " ");
				}
			}
			System.out.println();
		}
	}
}
