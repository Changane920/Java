package Operators;

public class Operator5 {
	//reverse number
	public static void main(String[] args) {
		int num = 321;
		int num1,num2,num3;
		
		num1 = (num % 10) * 100;
		num /= 10;
		
		num2 = (num % 10) * 10;
		num /= 10;
		
		num3 = num;
		
		System.out.println("Reverse number of " + num + " is " + (num1 + num2 + num3));
	}
}
