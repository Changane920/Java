package Recursive_EG;

public class Fibonancci {
	public static void main(String[] args) {
		N_Recursive_Fibo(3);
	}
	
	static void N_Recursive_Fibo(int num) {
		int first = 1;
		int second = 1;
		int third = 0;
		
		while(num > 0) {
			third = first + second;
			first = second;
			second = third;			
			num--;
		}
		System.out.println(third);
	}
}
