package N_FL_EX;

public class N_FL_EX_16 {
	public static void main(String[] args) {
		for(int row = 1; row <= 5; row++) {
			for(int space = 2; space >= row; space--) {
				System.out.print(" ");
			}
			for(int star1 = row - 1; star1 <= row; star1++) {
				System.out.print("* ");
			}
			for()
			System.out.println();
		}
	}
}
