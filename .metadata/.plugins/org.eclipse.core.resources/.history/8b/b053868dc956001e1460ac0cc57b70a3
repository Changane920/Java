package WL_EX;

public class WL_EX_2 {
	public static void main(String[] args) {
		int num = 12345;
		int result, divide = 0, for_multiply = 1;
		int n1, n2, n3, n4, n5;
		while(num > 0) {
			divide = num / 10;
			for(int i = 5; i > 0; i++) {
				for_multiply *= 10;
				result = divide + for_multiply;
			}
		}
		System.out.println(result);
	}
}
