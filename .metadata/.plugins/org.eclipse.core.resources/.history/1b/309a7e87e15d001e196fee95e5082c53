package Game;

public class tictactoe {
	static String[] board;
	public static void main(String[] args) {
		board = new String[9];
		for(int i = 0; i < board.length; i++) {
			board[i] = String.valueOf(i+1);
		}
		printBoard();
	}
	
	static void printBoard() {
		for(int i = 1; i <= 7; i++) {
			for(int j = 1; j <= i; j++) {
				if(i == 1 || i == 7) {
					System.out.println("| --- | --- | --- |");
				}
				else {
					
				}
			}
		}
	}
}
