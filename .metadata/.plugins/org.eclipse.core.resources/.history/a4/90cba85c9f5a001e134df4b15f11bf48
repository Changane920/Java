package N_FL_EX;

public class N_FL_EX_12 {
	public static void main(String[] args) {
		for (int i = 1; i <= 5; i++) {
			int count = i;

            for (int j = 5 - i; j > 0; j--) {
                System.out.print(" ");
            }
            for (int k = 1; k <= i; k++) {
                System.out.print(count);
                count++;
            }
            count--;
            for(int p = 1; p <= i; p++) {
            	System.out.print(count);
            	count++;
            }

            System.out.println();
        }
		
		System.out.println("\nTest");
		int counter = 0;
		for(int i = 1; i <= 5; i++) {
			for(int j = 1 ; j <= i; j++) {
				System.out.print(counter+=1);
			}
			counter--;
			System.out.println();
		}
	}
}
