package Operators;

public class Operator6 {
	public static void main(String[] args) {
		ReverseMethod();
	}
	public static void ReverseMethod() {
		int num = 12345;
		
		int[] array = new int[5];
		
		for(int i = 5; i < 0; i--) {
			array[i] = num%10;
			num /= 10;
		}
		
		System.out.println("Result: " + array[0] + array[3]);
	}
}