package For_Loop;

public class FL_EX {
	public static void main(String[] args) {
		//for loop sum 1 to 100
		int result = 0;
		for(int i = 1; i <= 100; i++) {
			result += i;
		}
		System.out.println(result);
		
		// sum only even number from 1 to 100
		int even = 0;
		for(int i = 2; i <= 100; i+=2) {
			even += i;
		}
		System.out.println(even);
	}
}
