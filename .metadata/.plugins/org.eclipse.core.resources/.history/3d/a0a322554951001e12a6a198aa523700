package Operators;

public class Operator6 {
	public static void main(String[] args) {
		ReverseMethod();
	}
	public static void ReverseMethod() {
		int num = 12345;
		int num1 = 0,num2 = 0;

		num1 = num%10;
		
		for(int i = 4; i < 0; i--) {
			num /= 10;
			num2 = num%10;
		}
		
		System.out.println("Result" + num1 + num2);
	}
}