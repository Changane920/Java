package Operators;

public class Test {
	public static void main(String[] args) {
		final int max = 100;
		//can't assign another value to max
		//max = 12;
		System.out.println(max);
		
		for (int l = 0; l < 5; l++) {
			for (int i = 0; i < 3; i++) {
				for(int k = 0; k < 2; k++) {
					System.out.print("+");
				}
				System.out.print("x");
			}
			System.out.print("o");
		}
	}
}
