package N_FL_EG;

public class N_FL_EG_5 {
	public static void main(String[] args) {
		for(int i = 5; i >= 1; i--) {
			for(int j = 1; j <= i; j++) {
				System.out.print(j + " ");
			}
			System.out.println();
		}
		
		for(int i = 1; i >= 9; i++) {
			for(int j = i; j >= i; j++) {
				System.out.print(j + " ");
			}
			System.out.println();
		}
	}
}
