package Method_EG;

import java.util.Scanner;

public class M_EG_1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("How many time?");
		
		print(input.nextInt());
	}
	
	static void print(int x) {
		for(int i = 1; i <= x; i++) {
			System.out.println("Hello World!");			
		}
	}
}
