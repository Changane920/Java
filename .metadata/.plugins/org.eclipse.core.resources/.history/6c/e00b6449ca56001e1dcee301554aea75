package WL_EX;

public class WL_EX_2 {
	public static void main(String[] args) {
		int num = 12345;
		int divide = 0, for_multiply = 1;
		int result = 0;

		while(num > 0) {
			num %= 10;
			for(int i = 5; i > 0; i--) {
				for_multiply *= 10;
			}
			result = num * for_multiply;
		}
		System.out.println("HEllo");
		System.out.println(result);
	}
}
