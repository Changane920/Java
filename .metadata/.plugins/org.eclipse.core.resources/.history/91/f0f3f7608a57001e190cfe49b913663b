package N_FL_EG;

public class N_FL_EG_5 {
	public static void main(String[] args) {
		for(int i = 5; i >= 1; i--) {
			for(int j = 1; j <= i; j++) {
//				System.out.print(j + " ");
			}
//			System.out.println();
		}
		
		System.out.println("Test");
		for(int a = 1; a <= 9; a++) {
			for(int b = 1; b <= 5; b++) {
				for(int c = 1; c <= b; c++) {
					System.out.print(c + " ");
				}
				System.out.println();
				for(int d = 5; d >= b; d--) {
					System.out.println(b + " ");
				}
			}
		}
	}
}
