package Recursive_EG;

public class Fibonancci {
	public static void main(String[] args) {
		N_Recursive_Fibo(6);
	}
	
	static void N_Recursive_Fibo(int num) {
		int first, second, third;
		while(num > 0) {
			first = 1;
			second = 1;
			third = first + second;
			System.out.print(first + " " + second + " " + third);
			first = second;
			second = third;
			
			num--;
		}
	}
}
